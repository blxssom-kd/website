    from github import Github

    # Replace with your token and repository name
    g = Github(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)
    repo = g.get_user().get_repo(PyGithub)

    file_path = "example.txt"
    file_content = "Hello, GitHub!"
    commit_message = "Add example.txt"

    try:
        contents = repo.get_contents(file_path, ref="main")
        repo.update_file(file_path, commit_message, file_content, contents.sha, branch="main")
        print(f"File {file_path} updated successfully")
    except:
        repo.create_file(file_path, commit_message, file_content, branch="main")
        print(f"File {file_path} created successfully")
