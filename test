    from github import Github

    # Replace with your token and repository name
    g = Github(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)
    repo = g.get_user().get_repo(PyGithub)

    file_path = "blxssom-kd/website/welcome.test.txt"
    file_content = "Hello, GitHub!"
    commit_message = "Add example.txt"

    try:
        contents = repo.get_contents(blxssom-kd/website/welcome.test, ref="main")
        repo.update_file(blxssom-kd/website/welcome.test, commit_message, file_content, contents.sha, branch="main")
        print(f"File {blxssom-kd/website/welcome.test} updated successfully")
    except:
        repo.create_file(blxssom-kd/website/welcome.test, commit_message, file_content, branch="main")
        print(f"File {blxssom-kd/website/welcome.test} created successfully")
